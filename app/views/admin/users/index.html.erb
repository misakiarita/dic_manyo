<h1>ユーザー 一覧画面(管理者)</h1>
<body>
<table class="table table-striped table-bordered">
<head>
<th>名前</th>
<th>メールアドレス</th>
</head>
<% @users.each do |t| %>
  <tr class="user_row" >
  <td><%= t.name %></td>
  <td><%= t.email %></td>
  <td><%= link_to '編集', edit_admin_user_path(t) %></td>
  <td><%= link_to '削除', admin_user_path(t), method: :delete, data: { confirm: '本当に削除しますか？' } %></td>
  </tr>
<% end %>
</table>

<%= link_to 'ユーザー追加', new_admin_user_path%>
</body>

